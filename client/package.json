        "eslint:fix": "eslint --fix . --ext js,graphql,ts,tsx --ignore-path .gitignore",
        "prettier": "prettier \"./**/*.{ts,tsx,js,graphql,yml,yaml}\" -l --ignore-path .gitignore",
