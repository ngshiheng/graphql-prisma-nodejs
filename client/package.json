        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/graphql": "^14.5.0",
        "@types/react-router-dom": "^5.1.3",
        "@types/yup": "^0.26.29",
        "concurrently": "^5.1.0",
        "prettier": "^1.19.1"
    },
    "prettier": {
        "trailingComma": "all",
        "tabWidth": 4,
        "singleQuote": true
